{
  "name": "resend-ai-sdk",
  "version": "0.0.0",
  "description": "Resend email tools for the AI SDK",
  "keywords": [
    "ai",
    "ai-sdk",
    "resend",
    "email",
    "tools"
  ],
  "homepage": "https://github.com/Flash-Brew-Digital/resend-ai-sdk",
  "bugs": {
    "url": "https://github.com/Flash-Brew-Digital/resend-ai-sdk/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ben Sabic",
    "url": "https://bensabic.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flash-Brew-Digital/resend-ai-sdk.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "test": "vitest run",
    "prepublishOnly": "pnpm build",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "bump-deps": "npx npm-check-updates -u -p pnpm && pnpm install",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "resend": "^6.9.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "ultracite": "7.1.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "ai": "^6.0.67",
    "zod": "^4.3.6"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.28.2"
}
